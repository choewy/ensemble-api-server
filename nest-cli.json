      "root": "apps/stream",
      "entryFile": "main",
      "sourceRoot": "apps/stream/src",
      "compilerOptions": {
        "tsConfigPath": "apps/stream/tsconfig.app.json"
      }
    },
    "typeorm": {
      "type": "library",
      "root": "libs/typeorm",
      "entryFile": "index",
      "sourceRoot": "libs/typeorm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/typeorm/tsconfig.lib.json"
      }
    },
    "health": {
